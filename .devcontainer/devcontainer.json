{
    "name": "GraphNLP Devcontainer",
    "build": {
        "context": "..",
        "dockerfile": "Dockerfile"
    },
    "settings": {
        "python.defaultInterpreterPath": "/opt/conda/bin/python",
    },
    "runArgs": [
        "--gpus=all"
    ],
    "extensions": [
        "ms-python.python",
        "github.copilot",
    ],
    "remoteUser": "devcontainer"
}